@M_KOJO_別人版_MESSAGE_SCOM_K23_7_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		PRINTFORMW
		RETURN 1
	;泥酔
	ELSEIF TCVAR:泥酔 == 1
		PRINTFORMW
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		IF TALENT:恋慕
			PRINTFORML 「っ、ぁぐ！？　ぉ、そんなにされては……つ、つぶれ……っ」
			PRINTFORMW 子宮内まで貫かんばかりに捻じ込まれた怒張の異様な感覚に、妖夢は目を白黒させている
		ELSE
			PRINTFORML 「ッ……ぎ！？　なっ、そんな深くまでは……到底……っ！」
			PRINTFORMW 子宮内までもを犯し抜こうと押し込まれた怒張が齎す違和感に妖夢は困惑している
		ENDIF
		RETURN 1
	ENDIF

	;基本セット
	IF EXP:子宮口開発経験 > 50
		IF TALENT:恋慕
			;種付けプレス
			IF TEQUIP:TARGET:体位 == 1 && (BEDROOM(CFLAG:PLAYER:現在位置) || (BATHROOM() && ITEM:20))
				;精液量1200以上
				IF EX:膣内精液 >= 1200
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「ぉ、あ……ぐHPH 折角、射精してっ、いただけたのにぃ……ひぐ、ぅあﾞああああHPH」","L"
							PRINTFORML たっぷりと注ぎ込まれて膨らんだ下腹部が%CALLNAME:PLAYER%の身体で押し潰され、ピストンの度に膣肉との寸隙から子種が噴き出す
							PRINTFORMW 逆流する粘液と最奥を責める剛直の感覚に圧倒された妖夢が、口元を汚す涎を意にも介さず悲鳴にも似た嬌声を上げた
						CASE 1
							CALL HPH_PRINT,@"「おﾞHPH おおぉHPH もっと……もっと、深くにぃHPH ……あぐぁ、ひぎぃHPH」","L"
							PRINTFORML 既に妊婦と見紛うばかりに腹部を膨張させつつも、悦楽の虜となった少女が貪婪に%CALLNAME:PLAYER%を求め続けている
							PRINTFORMW 胎をなみなみと子種で満たされてなお子宮口が亀頭に喰らい付き、抽挿に合わせて妖夢の腰が浮き上がっている……
						CASE 2
							CALL HPH_PRINT,@"「っ、はっ……か、ふっ……HPH しんじゃ、ぅ、ぐ……おﾞおっHPH」","L"
							PRINTFORML 膨れ上がった下腹部ごと身体を押し潰され、暴力的なまでの官能に曝されている妖夢は半ば酸欠に陥っているようだ
							PRINTFORMW それでも%CALLNAME:PLAYER%の背中に回された腕が緩む事はなく、薄れた理性を振り払い本能が際限無く快楽を求め続ける
					ENDSELECT
				ELSE
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「ぁう、かはっ……HPH こんなに、はげしく……嬉し、いぃ、ぅ、ああああぁHPH」","L"
							PRINTFORML %CALLNAME:PLAYER%に圧し掛かられ苦しげな息を吐きつつも、妖夢は陶然とした面持ちでこの状況を受け入れ切っている
							PRINTFORMW 子宮を潰さんばかりの苛烈な責めにただただ歓喜し、ぶつけられる官能のままに嬌声を上げる
						CASE 1
							CALL HPH_PRINT,@"「お……おﾞぉHPH ナカまで、きてるぅ……んぐ、はっ、ひいぃHPH」","L"
							PRINTFORML 奥の奥まで貫こうとする怒張を根元まで苦も無く沈め、解された子宮口が亀頭を呑み込み貪欲に喰らい付いてくる
							PRINTFORMW 寸毫の離別も惜しいと膣肉が強烈に吸い付き、抽挿に合わせて妖夢の腰が少し浮き上がっている……
						CASE 2
							CALL HPH_PRINT,@"「ぁ、%CALLNAME:PLAYER%さん、これ……HPH い、きが……っ、はっ、ひ、ぁ……っHPH」","L"
							PRINTFORML 太腿をがっしりと掴まれ半ば押し潰される形になっているにも拘らず、妖夢の呼吸を妨げているのは苦悶よりも快楽が勝るようだ
							PRINTFORMW %CALLNAME:PLAYER%の背中へしっかりと腕を回してしがみ付き、容赦のない打ち込みに恍惚と身体を震わせる
					ENDSELECT
				ENDIF
			;種付け交尾
			ELSEIF TEQUIP:TARGET:体位 == 2
				;精液量1200以上
				IF EX:膣内精液 >= 1200
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「ぎっ、あう……いﾞいぃHPH もう、むりぃ……えへ、えへへぇHPH」","L"
							PRINTFORML 遂に覚束なかった妖夢の四肢が自重を支えきれなくなり、腹ばいで%CALLNAME:PLAYER%に組み敷かれ只管子宮へと怒張を突き入れられる
							PRINTFORMW %CALLNAME:PLAYER%と\@ OUTROOF(CFLAG:現在位置) ? 地面 # 床 \@に挟まれ圧されている妖夢の秘裂からは、音を立てて白濁が噴き出し精液溜まりを作っている……
						CASE 1
							CALL HPH_PRINT,@"「あ、が……くっ、いぃいHPH そんなにぃ、突かれたら……も、もたなっ、ぃあﾞああぁHPH」","L"
							PRINTFORML とうに尋常ならざる量の子種を胎深くに吐き出されながらも肉欲に溺れてまぐわう様は、最早獣ですら目を剥くだろう
							PRINTFORMW 腕で上体を支えきれず、ほぼ崩れ落ちている妖夢のだらしなく開いた口元からは突かれる度に涎が流れ落ちている……
						CASE 2
							CALL HPH_PRINT,@"「おぁ、かっ、ふ……ぃHPH こっ、こんなの……バカになっちゃいますぅうﾞうHPH」","L"
							PRINTFORML 横溢する子種だけでは足りないと子宮口がずるりと亀頭を吞み込み、より深い部分で抽挿を玩味しようと嬉々として胎内までもを献じる
							PRINTFORMW 脳髄まで響くかのような法悦を叩き付けられた妖夢は、躰を痙攣させながら時折意識を飛ばしかけている……
					ENDSELECT
				ELSE
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「はぐぁ、ひっ、ああぁHPH た、たおれちゃ……なっ、ぃい、ひぃいん！HPH」","L"
							PRINTFORML 後ろから貫かれ子宮まで穿つ矯激な責めによがる妖夢は、ガクガクと震える四肢で辛うじて躰を支えている
							PRINTFORMW 悦楽に打ちひしがれながらも懸命に%CALLNAME:PLAYER%を受け止める姿に嗜虐心が鎌首を擡げ、腰使いを更に加速させていく……
						CASE 1
							CALL HPH_PRINT,@"「んうぅ、あぁ！HPH これじゃ、ほんとに……HPH っ、うあ、はっ……ぇHPH」","L"
							PRINTFORML 僅かな隙間も惜しんで互いの身体を押し付け合い情欲に浸る様は、既に獣同士の交尾に他ならない
							PRINTFORMW 興奮に任せた抽挿の余波で口元より覗いた妖夢の舌先から涎が一筋尾を引きながら垂れ落ちる……
						CASE 2
							CALL HPH_PRINT,@"「いっ、ぐ、うあぁ……おﾞ！？　な、ナカまで……はっ、ひっ……っHPH」","L"
							PRINTFORML 子宮ごと身体を穿つかのような強烈な一突きでいとも容易く亀頭が胎内に滑り込み、勢いのまま密着した尻肉が衝撃を物語る音を張り上げる
							PRINTFORMW 脳天まで貫くかのような悦楽をぶつけられた妖夢は、何とか意識を保とうと浅い呼吸を繰り返す……
					ENDSELECT
				ENDIF
			;それ以外
			ELSE
				;精液量1200以上
				IF EX:膣内精液 >= 1200
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「おぐ、かふっ、ぉ……あはっHPH %CALLNAME:PLAYER%さんのが、もうこんなに……HPH」","L"
							PRINTFORML 胎内で疼く夥しい量の精液が齎す圧迫感すら、妖夢にとっては官能を煽る一助でしかないようだ
							PRINTFORMW %CALLNAME:PLAYER%より下賜された子種の重さに酔い痴れ、少女は随喜に慄きながら色欲から生じた嬌音を奏でる
						CASE 1
							CALL HPH_PRINT,@"「おぉ、ご……あぐぅHPH うぁ……気持ちいぃ、コトしか……あはっ、えへ……HPH」","L"
							PRINTFORML 古い種汁を掻き出し新たな子種を植え付けようとする獣の如き抽挿が、妖夢の五感を忽ち快楽で塗り潰す
							PRINTFORMW %CALLNAME:PLAYER%への奉仕を至上の喜びと定めた雌穴が怒張を締め付け、餓えた子宮口が貪欲に喰らい付いてくる……
						CASE 2
							CALL HPH_PRINT,@"「あﾞあぁHPH %CALLNAME:PLAYER%さん、わたし、まだっ、イけますからぁHPH もっと……もっとぉHPH」","L"
							PRINTFORML それ自体が意思を持つかのように蠕動する媚肉が怒張を引き入れ、その都度の抽挿が子宮口を貫く形にされている
							PRINTFORMW 理性を手放しかけている妖夢は突かれる度に秘裂から白濁を噴き出し、%CALLNAME:PLAYER%から与えられる官能を一心不乱に享受している
					ENDSELECT
				ELSE
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「あんっ、ふああ、くぅっHPH すごい……%CALLNAME:PLAYER%さんのが、奥までぇHPH」","L"
							PRINTFORML 入念に慣らされ性感帯として躾けられた子宮口は、抉り込むような抽挿も易々と悦楽へ置換する
							PRINTFORMW %CALLNAME:PLAYER%に余す事なく性器を使われている幸甚に、妖夢は歓喜に打ち震えながら嬌声を上げている
						CASE 1
							CALL HPH_PRINT,@"「んんっ、ぐ……あうHPH うぅ……くるし、いのに……気持ちいいぃHPH」","L"
							PRINTFORML 膣肉を掻き分け子宮を押し潰さんばかりの激しいピストンが、苦痛を凌駕する性感を鮮明に妖夢へと刻み込む
							PRINTFORMW その報恩とばかりに膣襞が絶えず剛直を慰撫し続け、子種を求める子宮口が強烈に吸い付いてくる……
						CASE 2
							CALL HPH_PRINT,@"「んうぅHPH ぁ、%CALLNAME:PLAYER%さん、そんなに、されてはHPH 私もっ……ひぃいんHPH」","L"
							PRINTFORML 心地の良い膣肉へより深く怒張を沈めようとする欲念が%CALLNAME:PLAYER%の腰使いを激しいものへと押し進める
							PRINTFORMW 子宮口を荒々しく責め立てられ欲情を焚き付けられた妖夢は、%CALLNAME:PLAYER%に合わせて腰を動かし一層情痴に身を焦がしている
					ENDSELECT
				ENDIF
			ENDIF
			
			;今射精した
			IF NOWEX:PLAYER:11
				PRINTFORML
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT,@"「お、ぁあ……いぃ、いいですっHPH そのまま、そのまま一番奥でぇ……！HPH」","W"
						CASE 1
							CALL HPH_PRINT,@"「くっ、ううぁ……ぁ……っHPH が、ガマンしないでっ……大丈夫ですからぁHPH」","W"
						CASE 2
							PRINTFORMW 限界まで高まった射精感に委ね、%CALLNAME:PLAYER%は絶頂の瞬間渾身の一突きで妖夢の子宮を貫いた
					ENDSELECT
			ENDIF
		ELSE
			PRINTDATAL
				DATAFORM 「なっ、なにが……い、嫌……苦しい、だけじゃ……うぐっ、ああぁ！」
				DATAFORM 「ん、んんぅ！　……ぇ……ち、違うっ、私の身体、なのにぃ……ッ！」
			ENDDATA
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML %CALLNAME:PLAYER%によって開発され切った子宮口は苛烈な抽挿をものともせず、衝撃を難無く官能に変容させる
					PRINTFORMW その工程に本人の意思が介在する余地は無く、妖夢は望まぬ快楽にただ身を竦ませている……
				CASE 1
					PRINTFORML 突き穿たれる剛直の子宮を押し潰さんとする感触に、妖夢は苦痛と共に明確な快感を覚えているようだ
					PRINTFORMW 確かに熱を帯びている少女の吐息にほくそ笑み、%CALLNAME:PLAYER%はより深い部分へと己の徴を刻んでいく
			ENDSELECT
		ENDIF
	ELSEIF EXP:子宮口開発経験 >= 25
		IF TALENT:恋慕
			SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「ぉ、うあ……ぁ……く、苦しいのに、なんだか……いいぃHPH」","L"
				CASE 1
					CALL HPH_PRINT,@"「ぅぐっ、そ、そんなに奥まで、挿れたら……つぶれちゃいますよぉHPH」","L"
			ENDSELECT
			PRINTFORMW 時間をかけて%CALLNAME:PLAYER%に慣らされた子宮口が与えられる刺激を快楽へと置換し始めたようだ
		ELSE
			PRINTDATAL
				DATAFORM 「ぐ……ぅ……こ、こんなの……苦しいだけの筈……ッ」
				DATAFORM 「ぎっ……う、んぅ……ぇ……な、なんで……？」
			ENDDATA
			PRINTFORMW %CALLNAME:PLAYER%にしつこく責められた子宮口が妖夢に苦痛以外の感覚を伝え始めている
		ENDIF
	ELSE
		IF TALENT:恋慕
			PRINTDATAL
				DATAFORM 「うぐ、ぅ……さ、流石に、ちょっと……苦しいです……」
				DATAFORM 「か、はっ……ぅ、疑う訳ではないのですが……これ、本当に大丈夫……ぐ、ぁあ……っ」
			ENDDATA
			PRINTFORMW まだ柔軟性を欠いた子宮口で快感を得るのは難しいだろう、気長に開発していく他なさそうだ
		ELSE
			PRINTDATAL
				DATAFORM 「っ……ぐ……こ、こっちの事も考えて……！」
				DATAFORM 「がっ……は……どうしてっ、こんな目にぃ……！」
			ENDDATA
			PRINTFORMW 妖夢は苦痛しか感じていないようだが、抵抗を削ぐという側面では寧ろ好都合だ
		ENDIF
	ENDIF
